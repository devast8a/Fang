trait Shape {
    fn sides(shape: Shape) -> u32
}

struct Triangle impl Shape {
    fn sides(self) -> u32 {
        return 3
    }
}

struct Square impl Shape {
    fn sides(self) -> u32 {
        return 4
    }
}

fn getSides(shape: Shape) -> u32 {
    return shape.sides()
}

fn test() -> void {
    getSides(Triangle{})
    getSides(Square{})
}