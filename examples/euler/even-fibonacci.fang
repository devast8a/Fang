# Sum all numbers between 1 and 1000 that are a multiple or 3 and/or 5
fn sumEvenFibonacci() -> u32 {
    mut fib_0: u32 = 1
    mut fib_1: u32 = 2
    mut sum: u32 = 0

    while (fib_1 < 4_000_000) {
        if (fib_1 % 2 == 0) {
            sum += fib_1
        }

        val temp: u32 = 0
        temp = fib_0 + fib_1
        fib_0 = fib_1
        fib_1 = temp
    }

    return sum
}

fn main() -> u32 {
    printf("%d\n", sumEvenFibonacci())
    return 0
}