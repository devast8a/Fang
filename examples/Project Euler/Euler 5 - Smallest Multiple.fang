# TODO: Expose as a library and import
fn abs(value: Num) {
    if value >= 0 {
        return value
    } else {
        return -value
    }
}

fn gcd(a: Num, b: Num) {
    while b != 0 {
        val t = b
        b = a % b
        a = t
    }
    return a
}

fn lcm(a, b) {
    return abs(a * b) / gcd(a, b)
}