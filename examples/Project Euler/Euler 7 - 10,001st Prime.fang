val largest = 200_000
val primes = List{}
val sieve = List{2_000_000, 0}

for candidate in 2..largest {
    if sieve[candidate] == 0 {
        primes.push(candidate)

        # Fill in the sieve
        val i = candidate * 2
        while i < largest {
            sieve[i] = 1
            i = i + candidate
        }
    }
}

return primes[10_000]