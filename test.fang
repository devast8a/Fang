trait Trait {
    val field: int
    fn method()
}

class Concrete impl Trait {
    val field: int
    fn method() {
        writeLn("!!!\n", 0)
    }
}

fn makeConcrete(): Trait {
    val concrete: Concrete = Concrete{100}
    return concrete
}

fn main(): int {
    val value: Trait = makeConcrete()
    writeLn("%d\n", value.field)
    return 0
}