trait Trait {
    val field: int
    fn method()
}

class Concrete impl Trait {
    val field: int
    fn method() {
        writeLn("This is the method\n", 0)
    }
}

fn makeConcrete(): Trait {
    val concrete: Concrete = Concrete{100}
    return concrete
}

fn useTrait(value: Trait){
    writeLn("%d\n", value.field)
    value.method()
}

fn main(): int {
    val value: Trait = makeConcrete()

    writeLn("%d\n", value.field)
    value.method()

    useTrait(value)

    return 0
}